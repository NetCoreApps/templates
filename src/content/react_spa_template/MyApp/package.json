{
  "name": "MyApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "npm run build-vendor && npm run build",
    "dev": "webpack --config webpack.config.js --watch",
    "dev-server": "webpack-dev-server",
    "dtos": "cd src && typescript-ref",
    "build": "webpack --config webpack.config.js",
    "build-prod": "webpack --config webpack.config.js --env.prod",
    "build-vendor": "webpack --config webpack.config.vendor.js",
    "publish": "webpack --config webpack.config.vendor.js --env.prod && npm run build-prod && dotnet publish -c Release",
    "test": "jest",
    "test-watch": "jest --watch",
    "test-coverage": "jest --coverage"
  },
  "jest": {
    "globals": {
      "BaseUrl": "http://localhost:5000"
    },
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss|sass)$": "identity-obj-proxy",
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/test.fileMock.js"
    },
    "setupFiles": [
      "<rootDir>/src/test.shim.js",
      "<rootDir>/src/test.setup.js"
    ]
  },
  "dependencies": {
    "bootstrap": "^4.0.0-beta.2",
    "classnames": "^2.2.5",
    "enzyme-adapter-react-16": "^1.0.4",
    "es6-shim": "^0.35.3",
    "font-awesome": "^4.7.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router-dom": "^4.2.2",
    "@servicestack/client": "^1.0.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.3",
    "@types/jest": "^21.1.5",
    "@types/node": "^8.0.47",
    "@types/react": "^16.0.19",
    "@types/react-dom": "^16.0.2",
    "add-asset-html-webpack-plugin": "^2.1.2",
    "autoprefixer": "^7.1.6",
    "awesome-typescript-loader": "^3.2.3",
    "chalk": "^2.3.0",
    "clean-webpack-plugin": "^0.1.17",
    "copy-webpack-plugin": "^4.2.0",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "gulp": "^3.9.1",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^3.4.2",
    "jest": "^21.2.1",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.8",
    "precss": "^2.0.0",
    "react-test-renderer": "^16.0.0",
    "sass-loader": "^6.0.6",
    "@servicestack/cli": "^1.0.0",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.19.0",
    "ts-jest": "^21.1.4",
    "typescript": "^2.6.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3"
  }
}
